universe <- bitr(unique(AllData$geneID), fromType = "ENSEMBL",
                 toType = c("SYMBOL"),
                 OrgDb = org.Dm.eg.db)

universe  =  universe %>% dplyr::rename( geneID = ENSEMBL )

AllData = inner_join(AllData, universe) 
AllData$SYMBOL_unique = make.names(AllData$SYMBOL, unique = TRUE)



